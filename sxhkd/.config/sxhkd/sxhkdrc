XF86Audio{LowerVolume,RaiseVolume}
	pactl set-sink-volume 0 {-,+}5%	

XF86AudioMute
	amixer -q -D pulse sset Master toggle

XF86AudioPlay
	mpc toggle

XF86MonBrightness{Up,Down}
	light -{A,U} 10	

XF86ScreenSaver
	slock

Print
	maim -s /tmp/screenshot.png

super + c
	clipmenu -c -p "Clipboard" -l 20

super + p
	passmenu --type -c -p "Pass" -l 20

super + shift + p
	passmenu -c -p "Pass to Clip" -l 20

super + u
	mpdmenu :: -i -l 20

super + y
	mpdmenu -p :: -i -l 20

super + e
	dmenu_emoji

super + l
	slock

super + b
	statusbar

super + F1
	tty-clock.sh

super + F2
	cowsay_volume.sh

super + F3
	cowsay_battery.sh

super + F4
	cowsay_ip.sh

super + F5
	cowsay_temperatur.sh

super + F6
	cowsay_current_song.sh

super + F7
	cowsay_weather.sh

super + r
	dmenu_hist

alt + b
	$BROWSER

alt + m
	$TERMINAL -e $EMAILCLIENT

alt + p
	$TERMINAL -e $MUSICPLAYER

alt + t
	$TERMINAL -e $TOP

alt + shift + y
	mpv-youtube.bash -c -p "YouTube Bookmarks" -i -l 20

alt + y
	$TERMINAL -e youtube-viewer

alt + n
	$TERMINAL -e $RSSREADER

alt + w
	wifi

alt + a
	$TERMINAL -e abook

super + shift + Return
	cst
