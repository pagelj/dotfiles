	clipmenu -c -p "Clipboard" -l 20

super + p
	passmenu --type -c -p "Pass" -l 20

super + shift + p
	passmenu -c -p "Pass to Clip" -l 20

super + u
	mpdmenu :: -c -i -l 20

super + y
	mpdmenu -p :: -c -i -l 20

super + a
	st -e abook

super + e
	dmenu_emoji

